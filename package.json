{
  "name": "site_familial",
  "version": "1.0.0",
  "description": "Site familial",
  "main": "index.htm",
  "dependencies": {
    "browser-sync": "^2.29.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "node-sass --output-style compressed -o dist src/scss",
    "watch:css": "onchange \"src/scss\" -- npm run build:css",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "watch": "run-p serve watch:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoFram/site_familial.git"
  },
  "author": "Bertrand Frammery",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HugoFram/site_familial/issues"
  },
  "homepage": "https://github.com/HugoFram/site_familial#readme"
}
